{
	"jeebs": {
		"app": {
			"name": "Console App",
			"suite": "Rqlite"
		},
		"logging": {
			"minimum": "Debug",
			"overrides": {
				"System.Net.Http.HttpClient": "Warning"
			},
			"providers": {
				"console.serilog": {
					"enabled": true
				}
			}
		},
		"services": {
			"console": {
				"serilog": {
					"addPrefix": false
				}
			}
		}
	}
}